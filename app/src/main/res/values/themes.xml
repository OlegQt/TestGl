<resources>
    <!-- Base application theme. -->
    <style name="Theme.TestGl" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_blue_grey</item>
        <item name="colorPrimaryVariant">@color/light_blue_grey</item>
        <item name="colorOnPrimary">@color/text_icons</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/pink</item>
        <item name="colorSecondaryVariant">@color/purple_700</item>
        <item name="colorOnSecondary">@color/text_icons</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="bottomNavigationStyle">@style/CustomBottomNavigation</item>

    </style>
</resources>